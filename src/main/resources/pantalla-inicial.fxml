<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ComboBox?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
      alignment="CENTER"
      xmlns="http://javafx.com/javafx/8.0.162-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="PantallaInicialController">
    <TextField fx:id="fieldNombre"/>
    <ComboBox fx:id="comboColor"/>
    <Button text="Agregar Jugador" onAction="#agregarJugador"/>
    <Button text="Comenzar Juego" onAction="#comenzarJuego"/>
</VBox>
